{
  "extends": ["config:recommended"],
  "baseBranches": ["renovatebot"],
  "automerge": true,
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["(^|/)dagger\\.json$"],
      "matchStrings": [
        "\"engineVersion\":\\s*\"(?<currentValue>.*?)\""
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "dagger/dagger"
    }
  ]
}
