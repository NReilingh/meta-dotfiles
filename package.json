{
  "name": "meta-dotfiles",
  "module": "src/main.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "dev": "bun test --watch --coverage",
    "compile": "run-p compile:*",
    "compile:darwin-arm64": "bun build ./src/main.ts --compile --target=bun-darwin-arm64 --outfile build/bin/mf-darwin-arm64",
    "compile:darwin-x64": "bun build ./src/main.ts --compile --target=bun-darwin-x64 --outfile build/bin/mf-darwin-x64",
    "test": "bun test --coverage",
    "lint": "tsc --noEmit --noUnusedLocals --noUnusedParameters --pretty --skipLibCheck"
  },
  "peerDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@effect/cli": "^0.36.36",
    "@effect/platform-bun": "^0.36.1",
    "common-path-prefix": "^3.0.0",
    "effect": "^3.2.5",
    "xstate": "^5.13.0"
  }
}
